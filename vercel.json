{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://news-manager-chi.vercel.app"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/subscriber/subscribe",
      "dest": "src/index.js"
    },
    {
      "src": "/subscriber/unsubscribe",
      "dest": "src/index.js"
    },
    {
      "src": "/subscriber",
      "dest": "src/index.js"
    },
    {
      "src": "/newsletter/create",
      "dest": "src/index.js"
    },
    {
      "src": "/newsletter/:newsletterId",
      "dest": "src/index.js"
    },
    {
      "src": "/emailhandler/sendNewsletter",
      "dest": "src/index.js"
    }
  ]
}
