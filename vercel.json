{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/subscriber/subscribe",
      "dest": "src/index.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://news-manager-chi.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/subscriber",
      "dest": "src/index.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://news-manager-chi.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/subscriber/unsubscribe",
      "dest": "src/index.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://news-manager-chi.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/newsletter/:newsletterId",
      "dest": "src/index.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://news-manager-chi.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization",
        "Access-Control-Allow-Credentials": "true"
      }
    },
     {
      "src": "/newsletter/create",
      "dest": "src/index.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://news-manager-chi.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/emailhandler/sendNewsletter",
      "dest": "src/index.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://news-manager-chi.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization",
        "Access-Control-Allow-Credentials": "true"
      }
    },
  ]
}
